<p id="notice"><%= notice %></p>
<div class="col-12" id="notice">
  Shared Notes
</div>
<table class="table pb-3">
  <tr>
    <th>User</th>
    <th>Note</th>
  </tr>
  <% @notes.each do |note|%>
  <tr>
    <td>
      <%=note.user.name%>
    </td>
    <td>
      <%=note.title%>
    </td>
  </tr>

  <%end%>
</table>
<%= link_to 'View Notes', notes_path %> | <%= link_to 'Back', users_path %>
